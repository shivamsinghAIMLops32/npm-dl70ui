{
  "name": "dl70-ui",
  "version": "1.1.0",
  "description": "A beautiful, customizable, and accessible React toast notification component library",
  "license": "MIT",
  "author": "Shivam Singh",
  "type": "commonjs",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "es",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "toast",
    "notification",
    "alert",
    "ui",
    "component",
    "toast-notifications",
    "react-components",
    "dl70",
    "accessibility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shivamsinghAIMLops32/npm-dl70ui .git"
  },
  "bugs": {
    "url": "https://github.com/shivamsinghAIMLops32/npm-dl70ui/issues"
  },
  "homepage": "https://github.com/shivamsinghAIMLops32/npm-dl70ui#readme",
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "babel src --out-dir lib --copy-files",
    "build:es": "babel src --out-dir es --copy-files --env-name=esmodules",
    "clean": "rimraf lib es",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "prepublishOnly": "npm run clean && npm run lint && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-object-rest-spread": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.37.0",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 11"
  ],
  "dependencies": {
    "@babel/runtime": "^7.28.4"
  }
}
