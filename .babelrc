{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        },
        "modules": false
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-class-properties",
    "@babel/plugin-transform-object-rest-spread",
    [
      "@babel/plugin-transform-runtime",
      {
        "helpers": true,
        "regenerator": true,
        "useESModules": false
      }
    ]
  ],
  "env": {
    "cjs": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            },
            "modules": "cjs"
          }
        ],
        [
          "@babel/preset-react",
          {
            "runtime": "automatic"
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-transform-class-properties",
        "@babel/plugin-transform-object-rest-spread",
        [
          "@babel/plugin-transform-runtime",
          {
            "helpers": true,
            "regenerator": true,
            "useESModules": false
          }
        ]
      ]
    },
    "esmodules": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
            },
            "modules": false
          }
        ],
        [
          "@babel/preset-react",
          {
            "runtime": "automatic"
          }
        ]
      ],
      "plugins": [
        "@babel/plugin-transform-class-properties",
        "@babel/plugin-transform-object-rest-spread",
        [
          "@babel/plugin-transform-runtime",
          {
            "helpers": true,
            "regenerator": true,
            "useESModules": true
          }
        ]
      ]
    }
  }
}
